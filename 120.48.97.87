server {
	listen  8889;
	server_name  127.0.0.1;
	charset UTF-8;
	client_max_body_size 75M;
	location / { 
        	include uwsgi_params;                   
        	uwsgi_pass 127.0.0.1:8000;             
        	uwsgi_read_timeout 15;                  
    	}   	
}
server {
        listen       80;
        server_name  120.48.97.87;
	location /static {
 
            alias /root/project/backend/static;  #此处输入你的静态文件存放的文件夹路径
 
       }
	location / {
        	alias   /root/project/front/dist;
        	index  index.html index.htm;
        	try_files $uri $uri/ /index.html;
    	}

    	location /admin {
       	alias   /root/project/admin/dist/;
        	index  index.html index.htm;
        	try_files $uri $uri/ /admin/index.html;
    	}
	location /api {
       	proxy_pass http://120.48.97.87:8889/;
    	}
}



